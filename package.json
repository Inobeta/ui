{
  "name": "@Inobeta/ui",
  "version": "16.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --disable-host-check",
    "build": "ng build --configuration production",
    "test": "ng test  --code-coverage",
    "test-ci": "ng test  --code-coverage --watch=false",
    "test-with-diagnostic": "ng test --sourceMap=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json && cp -r ./src/app/inobeta-ui/themes ./dist/ && cp ./src/app/inobeta-ui/ui/kai-table/_table-theme.scss ./dist/src/app/inobeta-ui/ui/kai-table/_table-theme.scss",
    "release": "npm run packagr && cd dist/ && npm pack",
    "doc": "compodoc -p tsconfig.doc.json --disableCoverage --disableDomTree --disableGraph --hideGenerator --theme readthedocs",
    "pre-release": "npm version --no-git-tag-version prerelease",
    "storybook": "ng run ui:storybook",
    "build-storybook": "ng run ui:build-storybook",
    "patch": "npm version --no-git-tag-version patch",
    "minor": "npm version --no-git-tag-version minor",
    "major": "npm version --no-git-tag-version major"
  },
  "private": false,
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.10",
    "@angular/animations": "^16.2.12",
