    "@ngx-translate/http-loader": "^6.0.0",
    "jspdf": "^2.3.1",
    "jspdf-autotable": "^3.5.14",
    "papaparse": "^5.3.1",
    "rxjs": "6.6.7",
    "tslib": "^2.0.0",
    "xlsx": "^0.16.6",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular/cli": "~12.0.4",
