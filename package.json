    "@angular/compiler": "~8.0.1",
    "@angular/core": "~8.0.1",
    "@angular/flex-layout": "7.0.0-beta.24",
    "@angular/forms": "~8.0.1",
    "@angular/http": "^8.0.0-beta.10",
