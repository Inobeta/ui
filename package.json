    "ng-packagr": "^12.0.5",
    "ngrx-store-localstorage": "^12.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~4.1.0",
    "tslint": "~6.1.0",
    "typescript": "4.2.4"
