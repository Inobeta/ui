{
  "name": "@Inobeta/ui",
  "version": "15.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --disable-host-check",
    "build": "ng build --configuration production",
    "test": "ng test  --code-coverage",
    "test-ci": "ng test  --code-coverage --watch=false",
    "test-with-diagnostic": "ng test --sourceMap=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json && cp -r ./src/app/inobeta-ui/themes ./dist/",
    "release": "npm run packagr && cd dist/ && npm pack",
    "doc": "compodoc -p tsconfig.doc.json --disableCoverage --disableDomTree --disableGraph --hideGenerator",
    "pre-release": "npm version --no-git-tag-version prerelease",
    "patch": "npm version --no-git-tag-version patch",
    "minor": "npm version --no-git-tag-version minor",
    "major": "npm version --no-git-tag-version major"
  },
  "private": false,
  "dependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/animations": "^15.0.4",
    "@angular/cdk": "^15.0.3",
    "@angular/common": "^15.0.4",
    "@angular/compiler": "^15.0.4",
    "@angular/core": "^15.0.4",
    "@angular/forms": "^15.0.4",
    "@angular/material": "^15.0.3",
    "@angular/platform-browser": "^15.0.4",
    "@angular/platform-browser-dynamic": "^15.0.4",
    "@angular/router": "^15.0.4",
    "@Inobeta/flex-layout": "^15.0.0-inobeta",
    "@ngrx/effects": "^15.1.0",
    "@ngrx/store": "^15.1.0",
    "@ngrx/store-devtools": "^15.1.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "jspdf": "^2.3.1",
    "jspdf-autotable": "^3.5.14",
    "papaparse": "^5.3.1",
    "rxjs": "6.6.7",
    "tslib": "^2.4.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular/cli": "^15.0.4",
    "@angular/compiler-cli": "^15.0.4",
    "@angular/language-service": "^15.0.4",
    "@babel/preset-env": "^7.15.0",
    "@compodoc/compodoc": "^1.1.11",
    "@ngrx/schematics": "^15.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "^4.1.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "ng-packagr": "^15.0.3",
    "protractor": "~7.0.0",
    "ts-node": "~4.1.0",
    "tslint": "~6.1.0",
    "typescript": "4.8.4"
  },
  "publishConfig": {
    "@Inobeta:registry": "https://gitlab.com/api/v4/projects/8604184/packages/npm/"
  },
  "exports": {
    "default": {
      "sass": "./src/app/inobeta-ui/themes/default.scss"
    }
  }
