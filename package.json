{
  "name": "@Inobeta/ui",
  "version": "10.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build --prod",
    "test": "ng test  --code-coverage",
    "test-ci": "ng test  --code-coverage --sourceMap=true --watch=false",
    "test-with-diagnostic": "ng test --sourceMap=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json && cp -r ./src/app/inobeta-ui/themes ./dist/",
    "release": "npm run packagr && cd dist/ && npm pack",
    "compodoc": "compodoc -p tsconfig.app.json",
    "pre-release": "npm version --no-git-tag-version prerelease",
    "patch": "npm version --no-git-tag-version patch",
    "minor": "npm version --no-git-tag-version minor",
    "major": "npm version --no-git-tag-version major"
  },
  "private": false,
