    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "ng-packagr": "^18.1.0",
    "storybook": "^8.2.4",
    "ts-node": "~4.1.0",
    "typescript": "~5.5.3"
  },
